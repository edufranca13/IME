function sumb = sumbits (a, b, c)  if (a + b + c == 3)    sumb = [1 1];  elseif (a + b + c == 2)    sumb = [1 0];  elseif (a + b + c == 1)    sumb = [0 1];  else    sumb = [0 0];  endifendfunctionfunction sumS = sumSequence (num1, num2, startS, endS)  carryBit = 0;  sumS = "";    for i = startS : endS    partialSum = sumbits (str2num (num1 (startS + endS - i)), str2num (num2 (startS + endS - i)), carryBit);    carryBit = partialSum (1);    partialRes = num2str (partialSum (2));    sumS = strcat (partialRes, sumS);  endfor   endfunctionfunction s = sum (a, b)    #sinal da soma dos números  signal = sumbits (str2num (a (1)), str2num (b (1)), 0) (2);    #expoente da soma dos números  #tratar para overflow  exp = sumSequence(a, b, 2, 9);  disp (exp);    s = 0;endfunctiona = "100000001";b = "010010010";c = sum (a, b); 